		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#99F0C0"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				// "foreground": "#E0698A"
				"foreground": "#99F0C0"
				// "foreground": "#C2FF8E"
			}
